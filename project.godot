; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DDEM-Showcase"
run/main_scene="res://UtilityScenes/start_screen.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://UtilityScenes/icon.svg"

[autoload]

SceneManager="*res://addons/scene_manager/scene_manager.tscn"
Scenes="*res://addons/scene_manager/scenes.gd"
Inventory="*res://dino scripts/Inventory.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/finite_state_machine/plugin.cfg", "res://addons/scene_manager/plugin.cfg")
